{
  "name": "i18njs",
  "version": "2.0.0",
  "description": "A simple i18n for Javascript with a templating feature.",
  "main": "i18njs.js",
  "scripts": {
    "posttest": "npm run-script lint",
    "preversion": "npm test",
    "postversion": "npm run-script changelog",
    "test": "./node_modules/.bin/jasmine",
    "lint": "./node_modules/.bin/jscs i18njs.js",
    "changelog": "./node_modules/.bin/conventional-changelog -i CHANGELOG.md -w -b"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoannmoinet/i18njs.git"
  },
  "keywords": [
    "i18n",
    "translation",
    "template"
  ],
  "author": "Yoann Moinet <yoann.moinet@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoannmoinet/i18njs/issues"
  },
  "homepage": "https://github.com/yoannmoinet/i18njs",
  "devDependencies": {
    "conventional-changelog": "0.1.0-alpha.1",
    "jasmine": "^2.2.1",
    "jscs": "^1.13.1"
  }
}
